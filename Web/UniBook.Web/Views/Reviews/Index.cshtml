
<main class="main-content">
    <div class="container">
        <div class="page">
            @foreach (var link in Model)
            {
                <div class="embed-responsive embed-responsive-16by9 w-75 col-sm-3 ml-5">
                    <iframe class="embed-responsive-item" src="@link" allowfullscreen></iframe>
                </div>
            }
        </div>
    </div>
</main>